      </view>
      <view class="tubiao2-1">
        <image src="/images/mine/mine-2-4.png"></image>
        <text>储值订单</text>
      </view>
    </view>
  </view>
  <!-- 我的功能 -->
  <view class="gongneng">
    <view class="timu">我的功能</view>
    <view class="tubiao2">
      <view class="tubiao2-1">
        <image src="/images/mine/mine-3-1.png"></image>
        <text>我的地址</text>
      </view>
      <view class="tubiao2-1">
        <image src="/images/mine/mine-3-2.png"></image>
