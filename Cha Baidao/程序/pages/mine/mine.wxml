      </view>
      <view class="tubiao1-1">
        <view class="tubiao1up">
          <image src="/images/mine/mine-1-3.png"></image>
          <text>余额</text>
        </view>
        <view class="tubiao1down">
          <text class="down1">0</text>
          <text class="down2">元未使用</text>
