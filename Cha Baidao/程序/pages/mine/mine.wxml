      <view class="tubiao2-1">
        <image src="/images/mine/mine-3-4.png"></image>
        <text>联系客服</text>
      </view>
    </view>
  </view>
  <!-- 结束图标 -->
  <image class="jieshutubiao" src="/images/mine/mine-logo icon.png"></image>
  <text class="jieshuyu">第四小组技术支持</text>
