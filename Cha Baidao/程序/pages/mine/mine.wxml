      </view>
    </view>
  </view>
  <!-- 我的订单 -->
  <view class="dingdan">
    <view class="timu">我的订单</view>
    <view class="tubiao2">
      <view class="tubiao2-1">
        <image src="/images/mine/mine-2-1.png"></image>
        <text>食堂/外卖</text>
      </view>
      <view class="tubiao2-1">
        <image src="/images/mine/mine-2-2.png"></image>
